{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "quotes": "off",
        "no-param-reassign": "off",
        "no-underscore-dangle": "off",
        "import/no-extraneous-dependencies": "off",
        "max-len": "off"
    }
}
